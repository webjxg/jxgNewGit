.hideScroll {
  height: auto !important; }

html, body {
  height: 100%; }
body {
  font-family: "open sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 13px;
  color: #676a6c;
  table-layout:fixed
}
.form-horizontal {
  margin: 0 10px 10px; }
.table-bordered {
  border: none; }
.input-group-btn{
 width: 38px;
  height: 34px;
}

.input-group-btn .btn{
  width:100%;
  height:100%;
  color:#fff;
}

.input-group-btn i{
  font-size: 16px;

}
form table.dataTable.no-footer {
  border-bottom: none; }
.table-bordered > tbody > tr > td {
  /*width: 100px;*/
  border: 1px solid #e7e7e7;
  vertical-align: middle;
  word-wrap:break-word
}
td .form-control, .single-line {
  background-color: #FFFFFF;
  background-image: none;
  /*border: 1px solid #e5e6e7;*/
  color: inherit;
  display: block;
  padding: 6px 12px;
  transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
  width: 100%;
  border-radius: 2px;
  font-size: 14px; }
.help-inline{
  display: inline-block;
  margin-top:5px;
}

.table .input-group{
  width:100%;
}
.table textarea{
  resize: none;
}
.width-15 {
  width: 15%;
  background:#f9f9f9 !important;
  font-size:14px;
}
.width-35{
  position: relative;
}
.thumbnail{
  border: none;
  margin: 0;
}
.thumbnail img{
  width: 100px;
  height: 100px;
  background: url("../images/headPortrait.png") no-repeat;
  background-size:100% 100%;
}
#nameImagePreview{
  display: inline-block;
  vertical-align: middle;
  position: relative;

}
#nameImagePreview img{
  height: 100px;
  width: 100px;
}

#filePicker{
 display: inline-block;
  vertical-align: middle;
  width: 76px;
  height: 30px;
  cursor: pointer;
}


.webuploader-pick{
  line-height: 24px;
  padding:0;
  color: #fff;
  display: block;
  border: 0 !important;
}

.webuploader-element-invisible{
  opacity: 0;
}

.table-bordered > tbody > .tableFoot > td{
  line-height: 20px;
  border: none;
  padding: 0;
}
.tableFoot > td >span{
  width:100%;
  padding:0 5px;
  text-align: right;
  display: inline-block;
  vertical-align:middle;
}
.tableFoot > td >.form-control{
  width: 70%;
  border: none;
  outline: none;
  box-shadow: none;
  text-align: left;
  display: inline-block;
  vertical-align:middle;
  padding:0 5px;
}
.tableFoot > td >.form-control[readonly]{
  background-color: transparent;
}
/*jxg*/



.width-35 {
  width: 35%;
}
.form-control:focus {
  border-color: #1ab394 !important;
  box-shadow: none !important; }
form > .table > tbody > tr > td, .table > tfoot > tr > td {
  border-top: 1px solid #e7eaec;
  line-height: 1.42857;
  padding: 8px;
  color: black;
  vertical-align: middle; }
.btn-primary{
  background-color: #1ab394;
  border-color: #1ab394;
}
.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .open .dropdown-toggle.btn-primary{
  background-color: #18a689;
  border-color: #18a689;
  color: #FFFFFF;
}


