* { margin: 0; padding: 0; }
html,body{height: 100%; line-height:24px;font-size:14px;font-family: "open sans", "Helvetica Neue", Helvetica, Arial, sans-serif;scrollbar-face-color:#ffffff;scrollbar-highlight-color:#ffffff;scrollbar-darkshadow-color:#ffffff;}
h1, h2, h3, h4, h5, h6 { font-size: 100%; font-weight: 500; margin: 0; padding: 0;}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }
audio, canvas, video { display: inline-block; *display: inline; *zoom: 1; }
table { border-collapse: collapse; border-spacing: 0; }
th { text-align: inherit; }
fieldset, img { border: 0;}
abbr, acronym { border: 0; font-variant: normal; }
del { text-decoration: line-through; }
address, caption, cite, code, dfn, em, th, var { font-style: normal; font-weight: 500; }
ol, ul { list-style: none; }
caption, th { text-align: left; }
i,code,b{font-weight:normal;font-style: normal;}
code{background:transparent;border:none;}
a,a:hover,a:focus,a:visited{text-decoration: none;}
iframe { display: block; border:none;}
select{height: 30px;line-height: 30px;font-size: 16px;border-radius: 5px;border-color: #acbad5;padding: 0 5px 0 5px;  }


::-webkit-scrollbar-track { background-color: red; }
::-webkit-scrollbar-thumb { background-color: #4aa85a; border-radius: 15px; }

.wrapper-white{min-height:100%;background: #fff;padding:10px;border-radius: 3px;}
.wrapper-gray{min-height:100%;background: #f3f3f3;padding:10px;border-radius: 3px;}

.hide { display: none; }
.show { display: block; }

.center{text-align:center  !important}
/*自动宽度*/
.autoWidth{width:1% !important;white-space:nowrap;}
/*禁用鼠标*/
.not-allowed,input.not-allowed{cursor: not-allowed !important;}

/*工具拦*/
.toolbar-form {}
.toolbar-btns {margin-bottom: 10px;}

/* 文本不换行，显示省略号  一行*/
.line1{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 多行，显示省略号   */
.line2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

/* form-control */
.form-inline .form-control{ width: 160px; border-radius: 3px; }
.form-control{height:30px;}
.form-control, .single-line {
  background-color: #FFFFFF;
  background-image: none;
  border-radius: 1px;
  color: inherit;
  display: block;
  padding: 0 12px;
  transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
  width: 100%;
  font-size: 14px;
  box-shadow: none; }

.form-control:focus, .single-line:focus {
  border-color: #1ab394 !important; }



/*ibox样式控制 start*/
.ibox-title {
  background-color: #ffffff;
  border-color: #e7eaec;
  border-image: none;
  color: inherit;
  margin-bottom: 10px;
  /*padding: 5px 15px 5px;*/
  padding:5px 0;
}
.ibox-title > h5 {
  text-overflow: ellipsis;
  float: left;
  font-weight: 800;
  color: black;
  padding-left: 10px;
  position: relative;
  font-size: 16px;
  height: 30px;
  line-height: 30px;
}
.ibox-title > h5:before{
  content: "";
  width: 4px;
  height: 20px;
  background: #1ab394;
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -10px;
}
.ibox-title > .ibox-tools {
  display: none;
  float: right;
  margin-top: 20px;
  margin-right: 8px;
  position: relative;
  padding: 0; }
.ibox-title > .ibox-tools a {
  cursor: pointer;
  margin-left: 5px;
  color: #c4c4c4; }

.ibox-content {
  clear: both;
  background-color: #ffffff;
  color: inherit;
  /*padding:8px 15px;*/
}
.ibox-content table.dataTable tbody td a{
  margin: 2px;
}
/*ibox样式控制 end*/

/*ibox里的标题*/
.titName {
  padding-left: 10px;
  position: relative;
  font-size: 16px;
  line-height: 30px;
  font-weight: bold;
}
.titName:before{
  content: "";
  width: 4px;
  height: 20px;
  background: #1ab394;
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -10px;
}

/*分页样式控制 start*/
.pagination{ margin:0; }
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover{
  background-color: #f4f4f4; border-color:#ddd; color:#333; }
.pagination>li>a:focus, .pagination>li>a:hover, .pagination>li>span:focus, .pagination>li>span:hover{
  color:#333; }
.page-list{ margin-left:10px; line-height: 24px; }
.pagination-detail{ display: none; margin:8px 0; font-size:14px; color:#333; }
.pagination-box{ margin-top:0; margin-left: 10px; }
.page-info{ padding:3px 0; }
.pagination-roll{ line-height: 0; }
#pag-sel{ width:60px; outline:none; }
/*分页样式控制 end*/

/* layer全局弹窗样式*/
body div.myclass, body div.myclass .layui-layer-content,body div.myclass iframe{ border-radius: 10px; }
.layui-layer-title{ background-color: #1ab394  !important; color: #fff3f3!important; }
.layui-layer-btn .layui-layer-btn0{ background-color: #ffa82d !important; color: #FFFFFF !important; border:0 !important; }
.layui-layer-btn{border-top:1px solid #ddd;}

/*select2*/
.select2-container--default .select2-selection--multiple { border-color: #ccc; }
.select2-container--default.select2-container--focus .select2-selection--multiple { border-color: #1ab394 !important; }
.selection .select2-selection.select2-selection--single{outline: none;height:30px;border-color:#ccc;}
.select2-container--default .select2-selection--single{outline: none;height:30px;border-color:#ccc;}
.select2-container--default:focus .select2-selection--single:focus{border-color:#1ab394;}
.select2-container--default .select2-selection--single .select2-selection__rendered{line-height: 30px;}
.select2 .selection:focus{border-color: #1ab394 ;  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6);}
.inputFocus,select{outline: none;border:1px solid #e5e6e7;background: #fff;padding: 4px 12px;height: 30px;border-radius: 6px;}
.inputFocus:focus,select:focus{border:1px solid #1ab394 !important;box-shadow: 0 0 3px #1ab394;}

/*背景颜色控制 start*/
.bg-fff { background: #fff; }
.bg-f3, .bg-gray{ background: #f3f3f3; }
/*背景颜色控制 end*/

/*字体颜色控制 start*/
.col-red { color: red !important; }
.col-999 { color: #999 !important; }
.col-ffa82d { color: #ffa82d !important; }
.col-1ab394 { color: #1ab394 !important; }
.col-51cd50 { color: #51cd50 !important; }
.col-62b5e9 { color: #62b5e9 !important; }
.col-62b519{ color: #62b5e9 !important; }
/*字体颜色控制 start*/

/*间距控制 start*/
.mg0{ margin:0 !important;}
.mgv-10 { margin:10px 0; }
.mgb-10 { margin-bottom: 10px; }
.mgb-15 { margin-bottom: 15px; }
.mgb-20 { margin-bottom: 20px; }
.mgr-30 { margin-right: 30px; }

.pag0, .pag-0{padding:0; }
.pag10, .pag-10{padding:10px; }
.pag15, .pag-15{padding:15px; }
.pag30, .pag-30 {padding: 30px; }

.pagV-15 {padding:0 15px; }
.pagH-15 {padding:15px 0; }

.pagt-5{padding-top:5px; }
.pagt-10{padding-top:10px; }

.pagb-5{padding-bottom:5px; }
.pagb-10{padding-bottom:10px; }
/*间距控制 end*/

/*按钮控制 start*/
.btn {
  margin: 0 5px;
  height: 30px;
  padding: 0 5px;
  background: #f2f2f2;
  color: #000;
  line-height: 28px;
  min-width:60px;
}
.btn.active.focus, .btn.active:focus, .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus{
  outline: none;
}
/*btn中的icon图标*/
.btn i{
  display: inline-block;
  width: 16px;
  height: 15px;
  font-size: 13px;
  background-image: url("../images/icon.png");
  background-repeat: no-repeat;
  vertical-align: middle;
}
.btnaddr{
  background-color: #1ab394;
  color: white;
}
.btn-white{
  font-family: Microsoft YaHei !important;
  color: inherit;
  background: white;
  font-size: 13px;
  border: 1px solid #e7e7e7;
  /*min-width:55px;*/
}
.btn-white:hover, .btn-white:focus, .btn-white:active,  .btn-white.active, .ibox-content .open .dropdown-toggle.btn-white {
  color: #80afa7;
  box-shadow: 0 3px 1px 1px gainsboro !important;
}

.btn-search-input{
  height: 30px;
  padding: 0 10px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}
.btn-search-input i{
  font-size: 14px;
  vertical-align: middle;
}
.btn-primary {
  background-color: #1ab394;
  border-color: #1ab394;
  color: white;
}
.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .open .dropdown-toggle.btn-primary{
  background-color: #18a689;
  border-color: #18a689;
  color: #FFFFFF;
}
.btn-primary1 {
  padding: 4px;
  width: 10px !important;
  height: 20px !important;
  border: 1px solid #1ab394 !important;
  color: #1ab394;
  background-color: transparent;
  border-radius: 3px; }
