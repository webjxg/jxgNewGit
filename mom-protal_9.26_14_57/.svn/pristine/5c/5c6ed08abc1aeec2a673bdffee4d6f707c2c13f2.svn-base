/*自定义勾选框样式 start*/
.form-checked {
  position: relative; }

.form-checked .isChecked, .form-checked .unCheck {
  color: #1ab394;
  font-size: 20px; }

.form-checked .unCheck {
  display: inline-block; }

.form-checked .isChecked {
  display: none; }

.form-checked input[type="checkbox"] {
  opacity: 0; }

.form-checked input[type="checkbox"]:checked + label .isChecked {
  display: inline-block; }

.form-checked input[type="checkbox"]:checked + label .unCheck {
  display: none; }

.form-checked label {
  position: absolute;
  width: 20px;
  height: 20px;
  top: 5px;
  left: 0; }

/*灰色选中*/
.gray-check-1 {
  color: #cacaca;
  font-size: 16px; }

.gray-check-1:before {
  content: "\f046"; }

/*灰色不选中*/
.gray-check-, .gray-check-0 {
  color: #cacaca;
  font-size: 16px; }

.gray-check-:before, .gray-check-0:before {
  content: "\f096"; }

/*自定义勾选框样式 end*/
.thumbnail {
  border: none;
  margin: 0; }

.thumbnail img {
  width: 100px;
  height: 100px;
  background: url("../images/headPortrait.png") no-repeat;
  background-size: 100% 100%; }

#nameImagePreview {
  display: inline-block;
  vertical-align: middle;
  position: relative; }

#nameImagePreview img {
  height: 100px;
  width: 100px; }

#filePicker {
  display: inline-block;
  vertical-align: middle;
  width: 76px;
  height: 30px;
  cursor: pointer; }

.webuploader-pick {
  line-height: 24px;
  padding: 0;
  color: #fff;
  display: block;
  border: 0 !important; }

.webuploader-element-invisible {
  opacity: 0; }

.form-table {
  border: 1px solid #ddd; }

