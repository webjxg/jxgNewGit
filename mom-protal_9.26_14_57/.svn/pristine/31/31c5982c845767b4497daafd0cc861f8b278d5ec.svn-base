<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>事件监控</title>
</head>
<style>
    .apsType-linkage li{
        display: inline-block;
    }
    .col-1ab394{
        color: #1ab394;
    }
</style>
<body id="monitoring" class="wrapper-gray">
    <div class="wrapper-white">
    <div class="ibox">
        <div class="ibox-title">
            <h1 class="titName">事件监控</h1>
        </div>
        <div class="ibox-content">
            <div class="row">
                <div class="pull-left">
                    <div>
                        <label>
                            <ul class="apsType-linkage">
                                <li>

                                </li>
                                <li class="col-1ab394">
                                    <i class="fa fa-check-circle"></i>
                                   上报:<span></span>
                                </li>
                                <li class="col-ffa82d">
                                    <i class="fa icon-wait"></i>
                                    处理中:<span></span>
                                </li>
                                <li class="btn-cacel">
                                    <i class="fa fa-times-circle"></i>
                                    关闭:<span></span>
                                </li>
                            </ul>
                        </label>
                    </div>
                </div>
            </div>
            <!--创建表格-->
            <table id="treeTable" class="table table-striped table-bordered table-hover table-condensed dataTables-example dataTable tree_table">
                <thead>
                <tr>
                    <th>序号</th>
                    <th>事件</th>
                    <th>原因</th>
                    <th>上报人</th>
                    <th>上报时间</th>
                    <th>状态</th>
                </tr>
                </thead>
                <tbody id="treeTableBody">
                </tbody>
            </table>
            <!--分页显示-->
            <div class="clearfix pagination-box">
                <div class="page-info pull-left"></div>
                <div id="pagination" class="pagination-roll pull-right"></div>
            </div>
        </div>
    </div>
</div>
	</body>
	<script data-main="/js/module/proDis/monitoring" src="/js/zlib/require.js"></script>
</html>
