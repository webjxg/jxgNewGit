.width-47{
    width: 47%;

}
.width-6{
    width: 6%;

}
.config .tit>span{
    font-size: 16px;
    line-height: 30px;
}
.config .config-content{
    border: 1px dashed #E3E3E3;
    padding: 10px;
    margin: 10px 0;
    position: relative;
}
.config .handelUpDownbox{
    position: absolute;
    top: 50%;
    margin-top:-14px ;
    right:12px;
}
.config .handelUpDownbox>div{
    display: block;
    font-size: 18px;
    cursor: pointer;
}
.config .handelUpDownbox>div:hover{
    color: #1ab394;
}
.config .handelLeftRightbox{
    position: absolute;
    top: 50%;
    margin-top:-51px ;
    right:-80px;
}
.config .handelLeftRightbox .btn{
    margin: 14px auto;
    display: block;
}
.config ul{
    border: 1px solid #E3E3E3;
    overflow-y: auto;
    max-height: 400px;
    min-height: 200px;
}
.config ul.list3{
    width: calc(100% - 30px);
}
.config li{
    font-size: 14px;
    height: 40px;
    line-height: 40px;
    padding: 0 10px;
    border-bottom: 1px solid #E3E3E3;
    cursor: pointer;
}
.config li:nth-child(odd){
    background-color: #f5f5f6;
}

.config li:hover{
    background-color: #E3E3E3;
}
.config li.active{
    background-color: #E3E3E3;
}
.config li:last-child{
    border-bottom: 0;
}



.config .glyphicon{
    display: block;
}
.config .fa:before{
    margin:0 5px;
}

.config .form-inline .form-control{
    width: 120px;
}
.config .toolbar-form{
    margin-bottom: 10px;
}
