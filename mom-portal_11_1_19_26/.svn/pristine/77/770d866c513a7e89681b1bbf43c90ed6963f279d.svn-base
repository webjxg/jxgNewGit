/*===========æ‰€æœ‰åˆ†ç±»ç£è´´æ ·å¼ä¹¦å†™============*/
.box-groups{
	list-style-type: none;
	display: inline-block;
	background: #fff;
	box-shadow: -1px 2px 3px #dadada;
	position: relative;
}
.box-groups:hover{
	box-shadow: -2px 3px 3px #dadada;
}

.panel-box-groups{
	list-style-type: none;
	display: inline-block;
	background: #fff;
	position: relative;
}
.panel-box-groups:hover{
	border: 1px solid #e6e1e1;
	box-shadow: 0px 0px 4px #e6e1e1;
	border-radius: 4px;
}

/*==å¿«æ·å…¥å£==*/
.box-groups>a{
	display: block;
	width: 100%;
	height: 100%;
}
span.short_span{
	display: block;
	width:100%;
	height:100%;
	text-align: center;
	color:#6d7172;
	float: left;
	position: relative;
}
span.short_span>p{
	font-size: 12px;
	font-weight: 600;
	color: #6d7172;
	margin:0px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
span.short_span>img{
	display: inline-block;
	width:56%;
	height:56%;
	margin:10% 0px 0px 0px;
}

/*æ•°å­—ç±»*/
.box-number{
	/*float: left;*/
	display: block;
}
.imgcorteg{
	min-height:85px;
	width:40%;
	display: block;
	overflow: hidden;
}
.imgcorteg>img{
	display: inline-block;
	width: 90%;
	margin: 13% 0px 0px 5%;
}
.comant{
	height:100%;
	box-sizing: border-box;
	position: relative;
	font-size:0;
}
.number-icon,.number-data{
	display: inline-block;
	vertical-align: middle;
	width:50%;
	overflow: hidden;
}
.comant>p{
	display: inline-block;
	font-size: 18px;
	font-weight: bold;
	color: #000;
}
.comant>span{
	font-size:12px;
	color: #6d7172;
	font-weight:600;
}
.upnumber{
	display: inline-block;
	width: 30%;
	height: 46%;
	position: absolute;
	right: 0px;
	top: 25%;
}
.function img.view-picture{
	/*width:50%;*/
	height:100%;
	display: inline-block;
	vertical-align: middle;
	padding:5%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;

}
.information{
	text-align: left;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size:20px;
	display: inline-block;
	vertical-align: middle;
	width:50%;
	color: #2e9fec;
	padding-right: 5px;
	transform: scaleY(2);
	font-weight: bold;
	text-align: center;
}
.number-content{
	height:100%;
