/*=====type=file æ ·å¼ä¿®æ”¹======*/
.file-box{
	position: absolute;
	width: 100%;
	height: 100%;
	float: left;
	overflow: hidden;
	top: 0px;
	left: 0px;
}
.file-box>input{
	float:left;
}
.yl_txt{
	width: 75%;
	padding: 0px 0px;
	overflow: hidden;
	white-space: nowrap;
	height: 100%;
	border: 1px solid #ccc;
	border-bottom-right-radius: 4px;
	border-top-right-radius: 4px;
	border-left: none;
	outline:none;
}
.yl_btn{
	width: 25%;
	height: 100%;
	line-height: 29px;
	padding: 0px 3px;
	text-align: center;
	background-color: #f5f5f5;
	border: 1px solid #ccc;
	border-bottom-left-radius: 4px;
	border-top-left-radius: 4px;
	border-right: none;
	outline: none;
}
.yl_file{
	width: 25%;
	height: 95%;
	position:absolute;
	top: 1px;
	left: 0px;
	padding:0px 0px;
	filter: alpha(opacity:0);
	opacity: 0;
	cursor: pointer;
	outline:none;
}
.star-icon{
	position: relative;
	top: 3px;
	margin-right: 3px;
	color: #f00;
}
.success{
	margin: 0 10px;
	height: 30px;
	width: 90px;
	border-radius: 3px;
	background: rgb(0,153,255);
	color: #FFFFFF;
}
.delete{
	margin: 0 10px;
	height: 30px;
	width: 90px;
	border-radius: 3px;
	background: #FFFFFF;
	color: rgb(245,103,103);
	border-color: rgb(245,103,103);
}
