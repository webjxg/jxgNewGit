html{
	font-size:12px;
	width:100%;
	height:100%;
}
*{margin:0;padding:0;}
a,a:hover,a:active,a:visited{
	text-decoration:none;
}
select{padding-left: 6px;}
input{padding-left: 10px;}
ul,li{list-style: none;margin: 0;padding: 0;}
body{
	overflow-x:hidden;
	width:100%;
	height:100%;
	min-height:300px;
	position:relative;
	background: #f3f3f4;
}
textarea{
	resize: none;
	resize: none\9;
	-ms-resize: none;
	-moz-resize: none;
	-webkit-resize: none;
}
/*select{*/
	/*-moz-appearance:none;*/
	/*-webkit-appearance:none;*/
	/*background:#fff url(img/select3.png) no-repeat right;*/
/*}*/
.pos-rel{
	position: relative;
}
.center{
	width:100%;
	padding-top: 10px;
	box-sizing:border-box;
	overflow-x:auto;
	min-height:100%;
	/*max-width:1280px; */
}

.block{
	width:48.034%;
	/*padding:3.778% 0;*/
	box-sizing:border-box;
	position:relative;
	list-style:none;
	float:left;
	background-color:#012d61;
	border:1px solid #0d80e0;
}
/*----åˆ†ç»„ä¿®æ”¹-----*/
.gridster-uls{
	position: relative;
	width: 100%;
	list-style:none;
	/*float:left;*/
	margin-bottom: 1.629% !important;
}
.function>li{
	float:left;
	position:relative;
}
.function>li:nth-of-type(4n){
	margin-right:0;
}
.function>li img{
	height:auto;
}
#fault,#health{
	margin-bottom:0;
}
#property,#health{
	margin-right:0;
}
#state{
	width:47.88%;
}
#property{
	width:46.92%;
}
#fault{
	width:22.67%;
	margin-right:1.563%;
}
#health{
	width:72.24%;
	position:relative;
	background: -webkit-linear-gradient(top,rgba(0,69,182,0.4),rgba(0,52,110,0.4));
}
/*=============================================================*/
.title{
	list-style:none;
	margin-left:1%;
	margin-top:0.5%;
	color:#00d2ff;
}
.title>li{
	float:left;
	margin-left:5px;
}
.title>li:last-child{
	font-size:1em;
}
.side{
	width:22.992%;
	list-style:none;
	margin-right:1%;
	color:#00d2ff;
	float:right;
	font-size:0.8em;
}
.side>li{
	float:left;
	background-color:#094880;
	padding:1% 2%;
	border-radius:10px;
	margin-left:3%;
	border:1px solid #02a4d3;
}
.side>li:nth-of-type(3){
	color:#fff;
	background-color:#044d96;
}
.yAxis{
	margin-left:-6.5%;
	margin-top:3.5%;
	list-style:none;
	color:#3f70af;
	font-size:0.6em;
	float:left;
}
.yAxis>li{
	margin-bottom:87%;
}
.pie{
	width:100%;
	height:77.8%;
	list-style:none;
	margin-top:5%;
	box-sizing:border-box;
}
.pie li{
	font-size:0.3em;
	width:4.338%;
	height:82%;
	float:left;
	border:1px solid #0c69bf;
	border-radius:5px;
	margin-left:4%;
	box-sizing:border-box;
	position:relative;
	color:#fff;
	text-align:center;
}
.pie li .back{
	display:inline-block;
	width:100%;
	text-align:center;
	position:absolute;
	left:0;
	bottom:0;
	border-radius:5px;
}
.pie1 .back,.pie5 .back{background-color:#ebca14;}
.pie2 .back,.pie3 .back,.pie4 .back,.pie6 .back{background-color:#1eb40e;}
.pie7 .back,.pie9 .back{background-color:#e32a2a;}
.pie8 .back,.pie10 .back{background-color:#14d4b7;}
.pie1{padding-top:3%;}
.pie2{padding-top:0px;}
.pie3{padding-top:0px;}
.pie4{padding-top:1%;}
.pie5{padding-top:4%;}
.pie6{padding-top:1%;}
.pie7{padding-top:7%;}
.pie8{padding-top:2%;}
.pie9{padding-top:7.2%;}
.pie10{padding-top:2%;}
.pie1 .back{height:66%;}
.pie2 .back{height:87%;}
.pie3 .back{height:87%;}
.pie4 .back{height:80%;}
.pie5 .back{height:60%;}
.pie6 .back{height:78%;}
.pie7 .back{height:40%;}
.pie8 .back{height:73%;}
.pie9 .back{height:38%;}
.pie10 .back{height:73%;}
.grade{
	position:absolute;
	right:2%;
	top:22%;
	list-style:none;
	color:#01a8d4;
	font-size:0.6em;
}
.state{
	display:inline-block;
	width:103%;
	text-align:center;
	position:absolute;
	top:105%;
	left:0;
	font-size:0.2em;
	color:#5487cb;
}
.grade li{
	margin-bottom:10px;
	padding-left:10px;
}
.cBlock{
	display:inline-block;
	width:15px;
	height:15px;
	border-radius:3px;
	margin-right:10px;
}
.c1{background-color:#1eb40e;}
.c2{background-color:#14d4b7;}
.c3{background-color:#ebca14;}
.c4{background-color:#e32a2a;}
.g{
	display:inline-block;
}
/*=========================bootstraqæ ·å¼è¦†ç›–======================================*/
#pop{
	display:none;
	width:100%;
	height:100%;
	position:fixed;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
	background-color:rgba(0,0,0,0.7);
}
.pop{
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
	width:870px;
	height:600px;
	/*background:url(img/tcbg.png) 0 0 no-repeat;*/
	background-color:rgba(0,0,0,1);
}
.p-header{
	width:100%;
	height:69px;
	position:relative;
	/*background:url(img/tt.png) 0 0 no-repeat;*/
}
#step{
	list-style:none;
	color:#fff;
	font-size:1.2em;
	margin-left:28%;
	width:100%;
	height:100%;
}
#step>li{
	float:left;
	margin-right:75px;
	height:69px;
	line-height:69px;
	position:relative;
	cursor:pointer;
}
#step>li:hover{
	color:#00f0ff;
}
#step>li img{
	display:none;
}
#step>li.s-on{
	color:#00f0ff;
}
#step>li.s-on img{
	height:160%;
	width:auto;
	position:absolute;
	left:-42px;
	top:-20px;
	display:block;
}
#close{
	display:inline-block;
	color:#8bbdda;
	text-indent:1px;
	position:absolute;
	right:30px;
	bottom:-750%;
	cursor:pointer;
}

