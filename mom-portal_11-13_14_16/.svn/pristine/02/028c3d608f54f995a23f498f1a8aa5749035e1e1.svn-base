<body id="instructOperation" class="wrapper-gray">
    <div class="ibox">
        <div class="ibox-title">
            <div class="titName"><span>指令接收</span></div>
            <!--状态-->
            <div class="toolbar-form">
                <div class="form-inline">
                    日期 ：<span style="margin-right: 15px" class="date"></span>
                </div>
                <div class="form-inline">
                    班次 ：<span style="margin-right: 15px" class="updateDate"></span>
                </div>
                <div class="form-inline">
                    班组 ：<span class="groupName"></span>
                </div>
                <button class="btn  btn-search pull-right" id="search-btn" type="button">
                    <i class="fa fa-rss"></i>
                    接收
                </button>
            </div>
        </div>
        <div class="ibox-content">

           <!-- <div class="row distance">
                <div class="col-sm-12">
                    <div class="pull-left">
                        <div>
                            <span> 日期 : </span><span style="margin-right: 15px" class="date"></span>
                            <span> 班次 : </span><span style="margin-right: 15px" class="updateDate"></span>
                            <span> 班组 : </span><span class="groupName"></span>
                        </div>
                    </div>
                    <div class="pull-right">
                            <button class="btn bg-ffa82d btn-search btn-white" id="search-btn" type="button">
                                <i class="fa fa-rss"></i>
                                接收</button>
                    </div>
                </div>
            </div>-->
            <!--表格区-->
            <table id="treeTable" class="table table-striped table-bordered table-hover table-condensed dataTables-example dataTable tree_table">
                <thead class="treeTable">
                <tr>
                    <th>名称</th>
                    <th>工序</th>
                    <th>状态</th>
                    <th>下达时间</th>
                    <th>下达人</th>
                    <th>执行时间</th>
                    <th>执行人</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody id="treeTableBody">
                </tbody>
            </table>

            <!--分页显示-->
            <div class="clearfix pagination-box">
                <div class="page-info pull-left"></div>
                <div id="pagination" class="pagination-roll pull-right"></div>
            </div>
        </div>
    </div>
    <script data-main="/js/module/proDis/instructOperation" src="/js/zlib/require.js"></script>
